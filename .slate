# setup: echo "source ~/dotfiles/.slate" > ~/.slate
# I currently have shift-alt-ctrl-cmd bound to the semicolon key via karabiner
bind f:shift;alt;ctrl;cmd focus 'Finder'
bind q:shift;alt;ctrl;cmd focus 'Google Chrome'
bind w:shift;alt;ctrl;cmd focus 'Code'
bind e:shift;alt;ctrl;cmd focus 'iTerm2'
bind m:shift;alt;ctrl;cmd throw right resize
bind n:shift;alt;ctrl;cmd throw left resize
# fullscreen
bind u:shift;alt;ctrl;cmd:toggle move screenOriginX;screenOriginY screenSizeX;screenSizeY
